<div class="container">
  <%= flash[:notice] %>
  <h3 class="ml-5 mt-4 mb-4"><span class="border-bottom border-dark">マイページ</span></h3>


  <table class="table table-bordered contents-area mb-5">
    <tbody>
      <tr>
        <th scope="row", class="table-secondary col-3 text-left">ニックネーム</th>
        <td><%= @end_user.nickname %></td>
      </tr>
      <tr>
        <th scope="row", class="table-secondary col-3 text-left">一言自己紹介</th>
        <td><%= @end_user.introduction %></td>
      </tr>
      <tr>
        <th scope="row", class="table-secondary col-3 text-left">メールアドレス</th>
        <td><%= @end_user.email %></td>
      </tr>
    </tbody>
  </table>
    <div class="row d-flex align-items-center mb-2">
    <div class="col-1"><strong>登録情報</strong></div>
    <div class="col-3"><%= link_to "編集する", edit_end_user_path, class: "btn btn-success" %></div>
  </div>


</div>